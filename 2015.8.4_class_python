# A small example:

class TheThing(object):
	
	def __init__(self):  # maybe it's a constructor
		self.number = 0
	
	def some_function(self):
		print "Called"

	def add_me_up(self,more):
		self.number += more
		return self.number

a = TheThing()
b = TheThing()

a.some_function()
b.some_function()

print a.add_me_up(20)
print b.add_me_up(30)

print a.number
print b.number
