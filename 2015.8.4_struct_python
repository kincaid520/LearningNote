# import struct

# struct.pack
# struct.uppack
# struct.calcsize
# struct.pack_into
# struct.unpack_from

# ref:
# http://blog.csdn.net/JGood/article/details/4290158

# ex:
import struct

a=20
b=30

pac = struct.pack("ii",a,b) # "ii" means integer, integer
print len(pac)
print repr(pac)

s1,s2 = struct.unpack("ii",pac) # unpack it with integer & integer
print s1
print s2
